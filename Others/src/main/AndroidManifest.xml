        <activity android:name=".effectsbeauty.EffectsBeautyActivity" />
        <activity android:name=".streammixing.AutoMixerStartActivity" />
        <activity android:name=".superresolution.SuperResolutionActivity" />
        <activity
            android:name=".videoobjectsegmentation.VideoObjectSegmentationActivity"
